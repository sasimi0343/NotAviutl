	local p1 = {x = v[1]+width, y = v[2]+width, z = v[3]}
	local p2 = {x = v[1]+width, y = v[2]-width, z = v[3]}
	local p3 = {x = v[1]-width, y = v[2]-width, z = v[3]}
	local p4 = {x = v[1]-width, y = v[2]+width, z = v[3]}
	
	if (pp1 == nil) then
