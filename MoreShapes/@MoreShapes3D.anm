local range = obj.track2 - obj.track1
local start = obj.track2 / obj.track0

local kijunx = linepoints[1][1]
local kijuny = linepoints[1][2]
local kijunz = linepoints[1][3]

local did = false
local func_tion = load(
function()
	if (did) then return nil end
	did = true
	return func
end, "3DLineFunction")

for i=start,(range/obj.track0) do
	x = i * obj.track0
	local y = func_tion()
	
	local point = {0, 0, 0}
	if (xaxis == 0) then
		point[1] = x + kijunx
	elseif (xaxis == 1) then
		point[2] = x + kijuny
	else
		point[3] = x + kijunz
	end
	
	if (yaxis == 0) then
		point[1] = y + kijunx
	elseif (yaxis == 1) then
		point[2] = y + kijuny
	else
		point[3] = y + kijunz
	end
	
	table.insert(linepoints, point)
end

end

