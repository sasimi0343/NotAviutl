function()
	if (did) then return nil end
	did = true
	return func
end, "3DLineFunction")

for i=start,(range/obj.track0) do
	x = i * obj.track0
	local y = func_tion()
	
	local point = {0, 0, 0}
	if (xaxis == 0) then
		point[1] = x + kijunx
