@[3D]Rotate
--track0:X,-999,999,1,0.01
--track1:Y,-999,999,1,0.01
--track2:Z,-999,999,1,0.01
--check0:Direct

local avi3d = require("avi3d")

for k,v in pairs(geos) do
	if (obj.check0) then
		for _,v2 in pairs(v.surface) do
			local p1 = v2[1]
			local p2 = v2[2]
			local p3 = v2[3]
			local p4 = v2[4]
			
			local pp1 = avi3d.PointRotate(p1, obj.track0, obj.track1, obj.track2)
			local pp2 = avi3d.PointRotate(p2, obj.track0, obj.track1, obj.track2)
			local pp3 = avi3d.PointRotate(p3, obj.track0, obj.track1, obj.track2)
			local pp4 = avi3d.PointRotate(p4, obj.track0, obj.track1, obj.track2)
			
			p1 = { x = pp1[1], y = pp1[2], z = pp1[3] }
			p2 = { x = pp2[1], y = pp2[2], z = pp2[3] }
			p3 = { x = pp3[1], y = pp3[2], z = pp3[3] }
			p4 = { x = pp4[1], y = pp4[2], z = pp4[3] }
			
			v2[1] = p1
			v2[2] = p2
			v2[3] = p3
			v2[4] = p4
		end
	else
		v.rx = v.rx + obj.track0
		v.ry = v.ry + obj.track1
		v.rz = v.rz + obj.track2
	end
end

