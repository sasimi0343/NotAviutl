for k,v in pairs(geos) do
	for i=0,obj.track0 do
	
local ng = {

surface = v.surface,
shading = {},
ox = v.ox,
oy = v.oy,
oz = v.oz,
rx = v.rx,
ry = v.ry,
rz = v.rz

}
		
		ng.ox = rand(-obj.track1, obj.track1, i, seed+1)
		ng.oy = rand(-obj.track1, obj.track1, i, seed+2)
		ng.oz = rand(-obj.track1, obj.track1, i, seed+3)
		
