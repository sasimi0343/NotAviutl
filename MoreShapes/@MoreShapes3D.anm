	else
		table.insert(sur, {p1, pp1, pp2, p2, 50})
		table.insert(sur, {p2, pp2, pp3, p3, 100})
		table.insert(sur, {p3, pp3, pp4, p4, 50})
		table.insert(sur, {p4, pp4, pp1, p1, 25})
	end
	
	pp1 = p1
	pp2 = p2
	pp3 = p3
	pp4 = p4
end

